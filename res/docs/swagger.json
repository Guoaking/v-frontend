            }
          }
        }
      }
    },
    "/kyc/face/compare": {
      "post": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "description": "Upload two images for face comparison",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "Public"
        ],
        "summary": "Face comparison",
        "parameters": [
          {
            "type": "file",
            "description": "Source image",
            "name": "source_image",
            "in": "formData",
            "required": true
          },
          {
            "type": "file",
            "description": "Target image",
            "name": "target_image",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/api.FaceCompareSuccessResponse"
            }
          }
        }
      }
    },
    "/kyc/face/detect": {
      "post": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "description": "Upload an image for face detection",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "Public"
        ],
        "summary": "Face detection",
        "parameters": [
          {
            "type": "file",
            "description": "Face image",
            "name": "picture",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/api.FaceDetectSuccessResponse"
            }
          }
        }
      }
    },
    "/kyc/face/search": {
      "post": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "description": "Upload a face image to search similar faces",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "Public"
        ],
        "summary": "Face search",
        "parameters": [
          {
            "type": "file",
            "description": "Face image",
            "name": "picture",
