          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "Auth",
          "Public"
        ],
        "summary": "Refresh Access Token",
        "parameters": [
          {
            "description": "Refresh token request",
            "name": "request",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/api.RefreshTokenRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/api.SuccessResponse"
            }
          }
        }
      }
    },
    "/auth/token": {
      "post": {
        "description": "Use Client credentials to obtain access tokens",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
