          }
        }
      }
    },
    "/kyc/liveness/silent": {
      "post": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "description": "Upload an image for silent liveness detection",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "Public"
        ],
        "summary": "Silent liveness",
        "parameters": [
          {
            "type": "file",
            "description": "Image",
            "name": "picture",
            "in": "formData",
            "required": true
          },
          {
            "enum": [
              "zh",
              "zh-CN",
              "en",
              "en-US"
            ],
            "type": "string",
            "example": "zh-CN",
            "description": "Language",
            "name": "language",
            "in": "formData"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/api.LivenessSilentSuccessResponse"
            }
          }
        }
      }
    },
    "/kyc/ocr": {
      "post": {
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "description": "Upload ID card image for OCR recognition",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "Public",
          "OCR"
        ],
        "summary": "OCR recognition",
        "parameters": [
          {
            "type": "file",
            "description": "ID card image",
            "name": "picture",
            "in": "formData",
            "required": true
          },
          {
            "enum": [
              "thai",
              "vietnamese",
              "indonesian",
              "chinese",
              "english",
              "tagalog",
              "malay"
            ],
            "type": "string",
            "example": "thai",
            "description": "Language default:thai",
            "name": "language",
            "in": "formData"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/api.OCRSuccessResponse"
            }
          }
        }
      }
    }
