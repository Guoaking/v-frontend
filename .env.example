# Frontend env template
# Copy to .env.local (do NOT commit .env.local)

# Backend API
# Local dev default: http://localhost:8082/api/v1
# VM / cross-container: use a routable IP/hostname, e.g. http://192.168.64.4:8082/api/v1
VITE_API_BASE_URL=http://localhost:8082/api/v1

# Use mock backend
VITE_USE_MOCK=false
